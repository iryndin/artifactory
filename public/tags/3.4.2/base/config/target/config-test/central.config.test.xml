<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config xmlns="http://artifactory.jfrog.org/xsd/1.5.7" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jfrog.org/xsd/artifactory-v1_5_7.xsd">
    <serverName>mymy</serverName>
    <offlineMode>false</offlineMode>
    <fileUploadMaxSizeMb>100</fileUploadMaxSizeMb>
    <dateFormat>dd-MM-yy HH:mm:ss z</dateFormat>
    <addons>
        <showAddonsInfo>true</showAddonsInfo>
        <showAddonsInfoCookie>1417111367448</showAddonsInfoCookie>
    </addons>
    <security>
        <anonAccessEnabled>true</anonAccessEnabled>
        <anonAccessToBuildInfosDisabled>false</anonAccessToBuildInfosDisabled>
        <hideUnauthorizedResources>false</hideUnauthorizedResources>
        <passwordSettings>
            <encryptionPolicy>supported</encryptionPolicy>
        </passwordSettings>
        <ldapSettings>
            <ldapSetting>
                <key>ldap1</key>
                <enabled>true</enabled>
                <ldapUrl>ldap://blabla</ldapUrl>
                <autoCreateUser>true</autoCreateUser>
                <emailAttribute>mail</emailAttribute>
            </ldapSetting>
        </ldapSettings>
        <ldapGroupSettings/>
    </security>
    <backups>
        <backup>
            <key>backup1</key>
            <enabled>false</enabled>
            <cronExp>* * * * *</cronExp>
            <retentionPeriodHours>168</retentionPeriodHours>
            <createArchive>false</createArchive>
            <excludedRepositories/>
            <sendMailOnError>true</sendMailOnError>
            <excludeBuilds>false</excludeBuilds>
        </backup>
    </backups>
    <localRepositories>
        <localRepository>
            <key>local1</key>
            <description>local repo 1</description>
            <notes>note1</notes>
            <includesPattern>**/*</includesPattern>
            <excludesPattern></excludesPattern>
            <repoLayoutRef>layout</repoLayoutRef>
            <enableNuGetSupport>false</enableNuGetSupport>
            <enableGemsSupport>false</enableGemsSupport>
            <enableNpmSupport>false</enableNpmSupport>
            <enableDebianSupport>false</enableDebianSupport>
            <enablePypiSupport>false</enablePypiSupport>
            <enableDockerSupport>false</enableDockerSupport>
            <blackedOut>false</blackedOut>
            <handleReleases>true</handleReleases>
            <handleSnapshots>true</handleSnapshots>
            <maxUniqueSnapshots>0</maxUniqueSnapshots>
            <suppressPomConsistencyChecks>false</suppressPomConsistencyChecks>
            <propertySets/>
            <archiveBrowsingEnabled>false</archiveBrowsingEnabled>
            <snapshotVersionBehavior>unique</snapshotVersionBehavior>
            <localRepoChecksumPolicyType>client-checksums</localRepoChecksumPolicyType>
            <calculateYumMetadata>false</calculateYumMetadata>
            <yumRootDepth>0</yumRootDepth>
            <debianTrivialLayout>false</debianTrivialLayout>
        </localRepository>
        <localRepository>
            <key>local2</key>
            <description>local repo 2</description>
            <notes>note2</notes>
            <includesPattern>**/*</includesPattern>
            <excludesPattern>**/*</excludesPattern>
            <repoLayoutRef>layout</repoLayoutRef>
            <enableNuGetSupport>false</enableNuGetSupport>
            <enableGemsSupport>false</enableGemsSupport>
            <enableNpmSupport>false</enableNpmSupport>
            <enableDebianSupport>false</enableDebianSupport>
            <enablePypiSupport>false</enablePypiSupport>
            <enableDockerSupport>false</enableDockerSupport>
            <blackedOut>false</blackedOut>
            <handleReleases>true</handleReleases>
            <handleSnapshots>true</handleSnapshots>
            <maxUniqueSnapshots>0</maxUniqueSnapshots>
            <suppressPomConsistencyChecks>true</suppressPomConsistencyChecks>
            <propertySets/>
            <archiveBrowsingEnabled>false</archiveBrowsingEnabled>
            <snapshotVersionBehavior>unique</snapshotVersionBehavior>
            <localRepoChecksumPolicyType>client-checksums</localRepoChecksumPolicyType>
            <calculateYumMetadata>false</calculateYumMetadata>
            <yumRootDepth>0</yumRootDepth>
            <debianTrivialLayout>false</debianTrivialLayout>
        </localRepository>
    </localRepositories>
    <remoteRepositories/>
    <virtualRepositories/>
    <proxies>
        <proxy>
            <key>proxy1</key>
            <host>localhost</host>
            <port>0</port>
            <username>1111</username>
            <password>1111</password>
            <ntHost>nthost</ntHost>
            <defaultProxy>false</defaultProxy>
        </proxy>
    </proxies>
    <propertySets>
        <propertySet>
            <name>propertySet1</name>
            <visible>true</visible>
            <properties/>
        </propertySet>
        <propertySet>
            <name>propertySet2</name>
            <visible>false</visible>
            <properties/>
        </propertySet>
    </propertySets>
    <repoLayouts>
        <repoLayout>
            <name>layout</name>
            <artifactPathPattern>artifactPathPattern</artifactPathPattern>
            <distinctiveDescriptorPathPattern>true</distinctiveDescriptorPathPattern>
            <descriptorPathPattern>descriptorPathPattern</descriptorPathPattern>
        </repoLayout>
    </repoLayouts>
    <remoteReplications/>
    <localReplications>
        <localReplication>
            <enabled>true</enabled>
            <cronExp>0 0/7 * * * ?</cronExp>
            <syncDeletes>false</syncDeletes>
            <syncProperties>true</syncProperties>
            <repoKey>local1</repoKey>
            <url>http://momo.com</url>
            <socketTimeoutMillis>15000</socketTimeoutMillis>
            <username>user1</username>
            <password>password1</password>
            <enableEventReplication>true</enableEventReplication>
        </localReplication>
    </localReplications>
    <gcConfig>
        <cronExp>0 0 4 * * ?</cronExp>
    </gcConfig>
    <cleanupConfig>
        <cronExp>0 12 5 * * ?</cronExp>
    </cleanupConfig>
    <virtualCacheCleanupConfig>
        <cronExp>0 12 5 * * ?</cronExp>
    </virtualCacheCleanupConfig>
</config>
