<?xml version="1.0"?>

<project name="Artifactory">
    <skin>
        <groupId>org.jfrog.maven.skins</groupId>
        <artifactId>jfrog-sf</artifactId>
        <version>1.1.0</version>
    </skin>
    <bannerLeft>
        <name>jfrog</name>
        <src>http://www.jfrog.org/images/logos/jfrog.logo.64.gif</src>
        <href>http://www.jfrog.org</href>
    </bannerLeft>
    <bannerRight>
        <name>Artifactory</name>
        <src>http://www.jfrog.org/images/logos/artifactory.png</src>
        <href>index.html</href>
    </bannerRight>
    <poweredBy>
        <logo name="Jackrabbit" href="http://jackrabbit.apache.org"
              img="http://www.jfrog.org/images/logos/jackrabbitlogo.gif"/>
        <logo name="Spring" href="http://www.springframework.org/"
              img="http://www.jfrog.org/images/logos/spring_white.png"/>
        <logo name="Wicket" href="http://wicket.sourceforge.net"
              img="http://www.jfrog.org/images/logos/wicket-logo.png"/>
        <logo name="Jetty" href="http://www.mortbay.org/"
              img="http://www.jfrog.org/images/logos/jetty_logo.gif"/>
        <logo name="JIRA" href="http://www.atlassian.com/software/jira/"
              img="http://www.jfrog.org/images/logos/jira.png"/>
        <logo name="Confluence" href="http://www.atlassian.com/software/confluence/"
              img="http://www.jfrog.org/images/logos/confluence.jpg"/>
        <logo name="YourKitJavaProfiler" href="http://www.yourkit.com/"
              img="http://www.jfrog.org/images/logos/yourkit.gif"/>
    </poweredBy>
    <body>
        <breadcrumbs>
            <item href="http://wik.jfrog.org" name="jfrog"/>
            <item href="http://www.jfrog.org/sites/artifactory/1.3" name="Artifactory 1.3"/>
        </breadcrumbs>
        <links>
            <item name="Live Demo" href="http://repo.jfrog.org/artifactory/webapp/"/>
            <item name="Wiki" href="http://wiki.jfrog.org/confluence/display/RTF13"/>
            <item name="Issues" href="http://issues.jfrog.org/jira/browse/RTFACT"/>
        </links>

        <menu name="Help">
            <item name="User Guide"
                  href="http://wiki.jfrog.org/confluence/display/RTF/User+Guide"/>
            <item name="Live Demo" href="demo.html"/>
            <item name="Acknowledgements" href="acknowledgements.html"/>
        </menu>

        ${reports}
    </body>
</project>
