                                        <!-- old artifact id -->
                                        <exclude>xerces:xerces</exclude>
                                        <!-- wrong group id -->
                                        <exclude>org.apache.commons:commons-io</exclude>
                                    </excludes>
                                </bannedDependencies>
                                <requirePluginVersions>
                                    <phases>clean,deploy,site</phases>
                                </requirePluginVersions>
                                <requireJavaVersion>
