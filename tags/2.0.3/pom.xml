                                </requireJavaVersion>
                                <requireMavenVersion>
                                    <version>2.0.9</version>
                                </requireMavenVersion>
                            </rules>
                            <fail>true</fail>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
